{
  "name": "vk-logger",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "bin": "bin/index.js",
  "scripts": {
    "build": "tsc -d",
    "start": "node --enable-source-maps ./dist/index.js",
    "start:prod": "NODE_ENV=production  npm-run-all start",
    "start:dev": "NODE_ENV=dev npm-run-all start",
    "clear": "rimraf dist bin",
    "dev": "npm-run-all build start:dev",
    "decrypt": "node bin/decrypt.js",
    "build:cli": "tsc --build tsconfig.cli.json",
    "preinstall": "npm-run-all build build:cli"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/node": "^16.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "commander": "^8.0.0",
    "crypto-js": "^4.0.0",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.0",
    "typeorm": "^0.2.34",
    "vk-io": "^4.3.1"
  }
}
